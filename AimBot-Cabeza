game:GetService(wiQFpxUoydAbpVwQLvrzgNNMYwRgvXYqnRxUfKyjmcjDjLdTRf('')):SetCore(wiQFpxUoydAbpVwQLvrzgNNMYwRgvXYqnRxUfKyjmcjDjLdTRf(''), {
    Title = wiQFpxUoydAbpVwQLvrzgNNMYwRgvXYqnRxUfKyjmcjDjLdTRf(''),
